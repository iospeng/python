<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--
    namespace:名称空间
    id:sql语句的唯一标识
    resultType:返回结果的类型
-->
<mapper namespace="com.test.Mapper.UserMapper">
    <select id="selectAllUser" resultType="com.test.User">
        select * from user;
    </select>
</mapper>